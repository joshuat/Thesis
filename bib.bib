@article{ozeri_long_1999,
	title = {Long spin relaxation times in a single-beam blue-detuned optical trap},
	volume = {59},
	url = {http://link.aps.org/doi/10.1103/PhysRevA.59.R1750},
	doi = {10.1103/PhysRevA.59.R1750},
	abstract = {A scheme to trap atoms in a blue-detuned optical dipole trap, formed with a single laser beam and a holographic phase plate, is demonstrated. 105 rubidium atoms are trapped for (1/e lifetime) ∼300 msec, at temperatures of ∼24 {\textbackslash}mu K and a density of ∼7×1011atoms/cm3, for a trapping beam detuning of 0.1–30 nm. The time for a trapped atom to scatter one photon off the trapping beam is measured and found to be linear in the trapping laser detuning. At a detuning of 0.5 nm from resonance this time was measured to be ∼100 msec, indicating that the atoms are exposed on average only to ∼1/700 of the maximal light intensity in the trap. The use of a single laser beam allows for simple dynamical changes of the potential and large adiabatic compressions, while keeping the atoms mainly in the dark.},
	number = {3},
	journal = {Physical Review A},
	author = {Ozeri, R. and Khaykovich, L. and Davidson, N.},
	month = mar,
