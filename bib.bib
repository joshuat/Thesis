
@article{pritchard_cooperative_2010,
	title = {Cooperative {Atom-Light} Interaction in a Blockaded Rydberg Ensemble},
	volume = {105},
	url = {http://link.aps.org/doi/10.1103/PhysRevLett.105.193603},
	doi = {10.1103/PhysRevLett.105.193603},
	abstract = {By coupling a probe transition to a Rydberg state using electromagnetically induced transparency {(EIT)} we map the strong dipole-dipole interactions onto an optical field. We characterize the resulting cooperative optical nonlinearity as a function of probe strength and density. We demonstrate good quantitative agreement between the experiment and an N-atom cooperative model for N=3 atoms per blockade sphere and the n=60 Rydberg state. The measured linewidth of the {EIT} resonance places an upper limit on the dephasing rate of the blockade spheres of {\textless}110 {kHz.}},
	number = {19},
	journal = {Physical Review Letters},
	author = {Pritchard, J. D. and Maxwell, D. and Gauguet, A. and Weatherill, K. J. and Jones, M. P. A. and Adams, C. S.},
	month = nov,
	year = {2010},
	keywords = {Advanced Seminar, Rydberg},
	pages = {193603}
},

