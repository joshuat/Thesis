	author = {Kleine Büning, G. and Will, J. and Ertmer, W. and Klempt, C. and Arlt, J.},
	year = {2010},
	keywords = {Physics and Astronomy},
	pages = {117--123},
	file = {SpringerLink Full Text PDF:/home/joshuatorrance/.zotero/zotero/knisi0ra.default/zotero/storage/WTZUQVPN/Kleine Büning et al. - 2010 - A slow gravity compensated atom laser.pdf:application/pdf;SpringerLink Snapshot:/home/joshuatorrance/.zotero/zotero/knisi0ra.default/zotero/storage/7WE397VC/Kleine Büning et al. - 2010 - A slow gravity compensated atom laser.html:text/html}
