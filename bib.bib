},

@article{lee_raman_1996,
	title = {Raman Cooling of Atoms in an Optical Dipole Trap},
	volume = {76},
	url = {http://link.aps.org/doi/10.1103/PhysRevLett.76.2658},
	doi = {10.1103/PhysRevLett.76.2658},
	abstract = {We have Raman cooled sodium atoms below the photon recoil temperature in a novel type of blue-detuned optical dipole force trap. In this trap 4.5×10\${\textasciicircum}5\$ atoms have been cooled to an effective three dimensional temperature of 1.0 {\${\textbackslash}mu\$K} at a final density of 4×1011cm\${\textasciicircum}-3\$. No atoms were lost during the cooling process. The phase space density increased by a factor of 320 over the uncooled sample. This is the highest phase space density achieved by an all-optical cooling method.},
	number = {15},
