@article{miller_far-off-resonance_1993,
	title = {Far-off-resonance optical trapping of atoms},
	volume = {47},
	url = {http://link.aps.org/doi/10.1103/PhysRevA.47.R4567},
	doi = {10.1103/PhysRevA.47.R4567},
	abstract = {We confine {85Rb} atoms in an optical dipole force trap with a very large detuning from resonance of up to 65 nm. Confinement times of 200 ms, limited only by background-gas collisions, are obtained without additional cooling. A typical trap contains 1300 atoms at a temperature of 0.4 {mK} and a peak density of 8×1011 cm-3. We measure spontaneous photon scatter rates of the trapped atoms to be less than 1.6×103 s-1, corresponding to recoil heating rates below 0.6 {mK/s.} The far-detuned trap confines atoms with a strong, nearly conservative optical force and negligible atomic excitation.},
	number = {6},
	journal = {Physical Review A},
	author = {Miller, J. D. and Cline, R. A. and Heinzen, D. J.},
	month = jun,
	year = {1993},
	pages = {R4567--R4570}
},

@article{chu_experimental_1986,
	title = {Experimental Observation of Optically Trapped Atoms},
	volume = {57},
	url = {http://link.aps.org/doi/10.1103/PhysRevLett.57.314},
	doi = {10.1103/PhysRevLett.57.314},
	abstract = {We report the first observation of optically trapped atoms. Sodium atoms cooled below 10-3 K in "optical molasses" are captured by a dipole-force optical trap created by a single, strongly focused, Gaussian laser beam tuned several hundred gigahertz below the D1 resonance transition. We estimate that about 500 atoms are confined in a volume of about 103 {\textbackslash}mu m3 at a density of 1011-1012 cm-3. Trap lifetimes are limited by background pressure to several seconds. The observed trapping behavior is in good quantitative agreement with theoretical expectations.},
	number = {3},
