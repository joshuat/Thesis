	note = {{PMID:} 17001090},
	keywords = {Crystallization, Hydrolysis, Light, Mass Spectrometry, Microscopy, Fluorescence, Models, Molecular, Protein Conformation, Proteins, proteomics, Reverse Transcriptase Polymerase Chain Reaction, Scattering, Radiation, Trypsin, Ultracentrifugation, {X-Rays}},
